import java.util.*;
public class factorial {

	@SuppressWarnings("resource")
	public static void main(String[] args) {
		int f=1;
		int num;
		Scanner s= new Scanner(System.in);
		System.out.println("Enter the number")	;
		num=s.nextInt();
		for(int i=1;i<=num;i++)
		{
		f=f*i;	
		}
	System.out.println(f);
	}

}
